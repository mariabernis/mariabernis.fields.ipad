GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/MagicalRecord" "${PODS_ROOT}/Headers/Public/PQFCustomLoaders" "${PODS_ROOT}/Headers/Public/UIColor+FlatColors"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/MagicalRecord" -isystem "${PODS_ROOT}/Headers/Public/PQFCustomLoaders" -isystem "${PODS_ROOT}/Headers/Public/UIColor+FlatColors"
OTHER_LDFLAGS = -ObjC -l"Pods-MagicalRecord" -l"Pods-PQFCustomLoaders" -l"Pods-UIColor+FlatColors" -framework "CoreData" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods